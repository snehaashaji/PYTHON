dict1={'nine':9,'ten':10,'one':1,'two':2,'eight':8}
print("the dictionary is:\n",dict1)
l1=list(dict1.items())
l1.sort()
dict1=dict(l1)
print("\n dictionary in ascending order is\n",dict1)
l2=list(dict1.items())
l2.sort(reverse=True)
dict2=dict(l2)
print("\n dictionary in descending order is :\n",dict2)
